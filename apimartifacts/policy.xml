<policies>
	<inbound>
		<cors allow-credentials="true" terminate-unmatched-request="false">
			<allowed-origins>
				<origin>https://staging-apim-demo.developer.azure-api.net</origin>
			</allowed-origins>
			<allowed-methods preflight-result-max-age="300">
				<method>*</method>
			</allowed-methods>
			<allowed-headers>
				<header>*</header>
			</allowed-headers>
			<expose-headers>
				<header>*</header>
			</expose-headers>
		</cors>
	</inbound>
	<backend>
		<forward-request timeout="10" />
	</backend>
	<outbound />
	<on-error />
</policies>